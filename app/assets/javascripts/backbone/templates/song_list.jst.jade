each song in songs
  li.song.clearfix
    .thumbnail
      a(href="#watch/v/#{song.video_id}")
        span.video-thumb
          span.video-default
            span.video-clip
              img(src=song.image_url, alt='Thumbnail')
              span.valign
    .s-content
      h3.title
        a
          span= song.title
      .description
        span= song.description
